@startuml

package reference {
	class NodeRef {

	}

	class InstructionNodeRef {
		+ head : NodeHeadRef
	}

	class SimpleNodeRef {
		+ body: String;
	}

	class CompoundNodeRed {
		+ body: List<NodeRef>
	}

	class NodeHeadRef {
		+ id: String
		+ name: String
	}

	class TermNodeRef {
		+ term : String
		+ explanation: String
	}

	NodeRef <|-- TermNodeRef
	NodeRef <|-- InstructionNodeRef
	InstructionNodeRef <|-- SimpleNodeRef
	InstructionNodeRef <|-- CompoundNodeRed

	InstructionNodeRef "1" -> NodeHeadRef

}


@enduml