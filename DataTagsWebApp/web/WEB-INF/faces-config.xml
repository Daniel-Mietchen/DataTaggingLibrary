<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.1"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <navigation-rule>
        <from-view-id>/management/explanationDetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>UPDATE_OK</from-outcome>
            <to-view-id>/management/explanationList.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/META-INF/msg</base-name>
            <var>msg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleWiz</base-name>
            <var>bundleWiz</var>
        </resource-bundle>
    </application>
</faces-config>
